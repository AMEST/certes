{
  "name": "certes",
  "description": "",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "test": "nyc mocha",
    "test-d": "mocha",
    "coverage": "nyc report --reporter=html",
    "codecov": "nyc report --reporter=text-lcov > build/coverage.lcov",
    "postinstall": "cd src/vsts && npm install",
    "package": "npm run clean && npm run compile && tfx extension create -r ./src/vsts --output-path ./build/",
    "clean": "rimraf ./build/*.vsix",
    "compile": "tsc -p ."
  },
  "dependencies": {
    "vsts-task-lib": "^2.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.56",
    "@types/q": "^1.0.6",
    "codecov": "^3.0.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.0",
    "tfx-cli": "^0.4.11",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fszlin/certes.git"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary"
    ],
    "all": true
  }
}
